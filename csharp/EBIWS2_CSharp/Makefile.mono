# $Id$
# ======================================================================
#
# Makefile to build C# EBI Web Services clients using Mono on UNIX.
#
# ======================================================================

all: build

### Build ###
build: \
build-EbiWS \
build-ClustalW2CliClient \
build-EBeyeCliClient \
build-FastaCliClient \
build-KalignCliClient \
build-MafftCliClient \
build-MuscleCliClient \
build-NcbiBlastCliClient \
build-PsiBlastCliClient \
build-PsiSearchCliClient \
build-TcoffeeCliClient \
build-WSDbfetchCliClient \
build-WuBlastCliClient

build-EbiWS:
	cd EbiWS && $(MAKE) -f Makefile.mono

build-ClustalW2CliClient: build-EbiWS
	cd ClustalW2CliClient && $(MAKE) -f Makefile.mono

build-EBeyeCliClient: build-EbiWS
	cd EBeyeCliClient && $(MAKE) -f Makefile.mono

build-FastaCliClient: build-EbiWS
	cd FastaCliClient && $(MAKE) -f Makefile.mono

build-KalignCliClient: build-EbiWS
	cd KalignCliClient && $(MAKE) -f Makefile.mono

build-MafftCliClient: build-EbiWS
	cd MafftCliClient && $(MAKE) -f Makefile.mono

build-MuscleCliClient: build-EbiWS
	cd MuscleCliClient && $(MAKE) -f Makefile.mono

build-NcbiBlastCliClient: build-EbiWS
	cd NcbiBlastCliClient && $(MAKE) -f Makefile.mono

build-PsiBlastCliClient: build-EbiWS
	cd PsiBlastCliClient && $(MAKE) -f Makefile.mono

build-PsiSearchCliClient: build-EbiWS
	cd PsiSearchCliClient && $(MAKE) -f Makefile.mono

build-TcoffeeCliClient: build-EbiWS
	cd TcoffeeCliClient && $(MAKE) -f Makefile.mono

build-WSDbfetchCliClient: build-EbiWS
	cd WSDbfetchCliClient && $(MAKE) -f Makefile.mono

build-WuBlastCliClient: build-EbiWS
	cd WuBlastCliClient && $(MAKE) -f Makefile.mono

