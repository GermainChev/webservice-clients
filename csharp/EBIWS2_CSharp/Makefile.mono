clean-MafftCliClient:
	cd MafftCliClient && $(MAKE) -f Makefile.mono) clean

clean-MuscleCliClient:
	cd MuscleCliClient && $(MAKE) -f Makefile.mono) clean

clean-NcbiBlastCliClient:
	cd NcbiBlastCliClient && $(MAKE) -f Makefile.mono) clean

clean-PsiBlastCliClient:
	cd PsiBlastCliClient && $(MAKE) -f Makefile.mono) clean

clean-PsiSearchCliClient:
	cd PsiSearchBlastCliClient && $(MAKE) -f Makefile.mono) clean

clean-TcoffeeCliClient:
	cd TcoffeeCliClient && $(MAKE) -f Makefile.mono) clean

clean-WSDbfetchCliClient:
	cd WSDbfetchCliClient && $(MAKE) -f Makefile.mono clean

clean-WuBlastCliClient:
	cd WuBlastCliClient && $(MAKE) -f Makefile.mono clean

