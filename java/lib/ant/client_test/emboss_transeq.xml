<project name="EBIWS2-Java-Test-EMBOSS-transeq">
	<!-- Apache Axis 1.x -->
	<target name="axis1-test-emboss_transeq" description="Axis 1.x: EMBOSS transeq (SOAP) test">
		<property name="client.jar.axis1" value="${jar.dir}/EmbossTranseq_Axis1.jar" />
		<antcall target="test-emboss_transeq-help">
			<param name="client.jar" value="${client.jar.axis1}" />
		</antcall>
		<antcall target="test-emboss_transeq-params">
			<param name="client.jar" value="${client.jar.axis1}" />
		</antcall>
		<antcall target="test-emboss_transeq-paramdetail">
			<param name="client.jar" value="${client.jar.axis1}" />
		</antcall>
		<antcall target="test-emboss_transeq-sequence-file">
			<param name="client.jar" value="${client.jar.axis1}" />
		</antcall>
		<antcall target="test-emboss_transeq-sequence-dbid">
			<param name="client.jar" value="${client.jar.axis1}" />
		</antcall>
		<antcall target="test-emboss_transeq-sequence-stdin">
			<param name="client.jar" value="${client.jar.axis1}" />
		</antcall>
		<!-- antcall target="test-emboss_transeq-idlist-file">
			<param name="client.jar" value="${client.jar.axis1}" />
		</antcall -->
		<!-- antcall target="test-emboss_transeq-idlist-stdin">
			<param name="client.jar" value="${client.jar.axis1}" />
		</antcall -->
		<!-- antcall target="test-emboss_transeq-multifasta-file">
			<param name="client.jar" value="${client.jar.axis1}" />
		</antcall -->
		<!-- antcall target="test-emboss_transeq-multifasta-stdin">
			<param name="client.jar" value="${client.jar.axis1}" />
		</antcall -->
	</target>
	
	<!-- JAX-WS RI -->
	<target name="jaxws-test-emboss_transeq" description="JAX-WS RI: EMBOSS transeq (SOAP) test">
		<property name="client.jar.jaxws" value="${jar.dir}/EmbossTranseq_JAXWS.jar" />
		<antcall target="test-emboss_transeq-help">
			<param name="client.jar" value="${client.jar.jaxws}" />
		</antcall>
		<antcall target="test-emboss_transeq-params">
			<param name="client.jar" value="${client.jar.jaxws}" />
		</antcall>
		<antcall target="test-emboss_transeq-paramdetail">
			<param name="client.jar" value="${client.jar.jaxws}" />
		</antcall>
		<antcall target="test-emboss_transeq-sequence-file">
			<param name="client.jar" value="${client.jar.jaxws}" />
		</antcall>
		<antcall target="test-emboss_transeq-sequence-dbid">
			<param name="client.jar" value="${client.jar.jaxws}" />
		</antcall>
		<antcall target="test-emboss_transeq-sequence-stdin">
			<param name="client.jar" value="${client.jar.jaxws}" />
		</antcall>
		<!-- antcall target="test-emboss_transeq-idlist-file">
			<param name="client.jar" value="${client.jar.jaxws}" />
		</antcall -->
		<!-- antcall target="test-emboss_transeq-idlist-stdin">
			<param name="client.jar" value="${client.jar.jaxws}" />
		</antcall -->
		<!-- antcall target="test-emboss_transeq-multifasta-file">
			<param name="client.jar" value="${client.jar.jaxws}" />
		</antcall -->
		<!-- antcall target="test-emboss_transeq-multifasta-stdin">
			<param name="client.jar" value="${client.jar.jaxws}" />
		</antcall -->
	</target>

	<target name="test-emboss_transeq-help">
		<echo message="EMBOSS transeq (SOAP): usage/help message" />
		<java fork="true" failonerror="true" jar="${client.jar}">
			<jvmarg value="${jvm.extra.jars}" />
			<arg value="--help" />
		</java>
	</target>
	
	<target name="test-emboss_transeq-params">
		<echo message="EMBOSS transeq (SOAP): parameter names" />
		<java fork="true" failonerror="true" jar="${client.jar}">
			<jvmarg value="${jvm.extra.jars}" />
			<arg value="--params" />
		</java>
	</target>
	
	<target name="test-emboss_transeq-paramdetail">
		<echo message="EMBOSS transeq (SOAP): parameter details" />
		<java fork="true" failonerror="true" jar="${client.jar}">
			<jvmarg value="${jvm.extra.jars}" />
			<arg value="--paramDetail" />
			<arg value="codontable" />
		</java>
	</target>
	
	<target name="test-emboss_transeq-sequence-file">
		<echo message="EMBOSS transeq (SOAP): File input" />
		<java fork="true" failonerror="true" jar="${client.jar}">
			<jvmarg value="${jvm.extra.jars}" />
			<arg value="--email" />
			<arg value="${test.email}" />
			<arg value="test_data/EMBL_AB000204.fasta" />
		</java>
	</target>
	
	<target name="test-emboss_transeq-sequence-dbid">
		<echo message="EMBOSS transeq (SOAP): DB:ID input" />
		<java fork="true" failonerror="true" jar="${client.jar}">
			<jvmarg value="${jvm.extra.jars}" />
			<arg value="--email" />
			<arg value="${test.email}" />
			<arg value="EMBL:AB000204" />
		</java>
	</target>
	
	<target name="test-emboss_transeq-sequence-stdin">
		<echo message="EMBOSS transeq (SOAP): Input sequence from STDIN and result to STDOUT" />
		<java fork="true" failonerror="true" jar="${client.jar}" input="test_data/EMBL_AB000204.fasta">
			<jvmarg value="${jvm.extra.jars}" />
			<arg value="--email" />
			<arg value="${test.email}" />
			<arg value="--outformat" />
			<arg value="out" /> <!-- Output -->
			<arg value="--outfile" />
			<arg value="-" /> <!-- Result output to STDOUT -->
			<arg value="-" /> <!-- Input sequence from STDIN -->
		</java>
	</target>
	
	<target name="test-emboss_transeq-idlist-file">
		<echo message="EMBOSS transeq (SOAP): ID list input file" />
		<java fork="true" failonerror="true" jar="${client.jar}">
			<jvmarg value="${jvm.extra.jars}" />
			<arg value="--email" />
			<arg value="${test.email}" />
			<arg value="--outformat" />
			<arg value="out" /> <!-- Output -->
			<arg value="--outfile" />
			<arg value="-" /> <!-- Result output to STDOUT -->
			<arg value="@test_data/embl_id_list.txt" />
		</java>
	</target>
	
	<target name="test-emboss_transeq-idlist-stdin">
		<echo message="EMBOSS transeq (SOAP): ID list input from STDIN and result to STDOUT" />
		<java fork="true" failonerror="true" jar="${client.jar}" input="test_data/embl_id_list.txt">
			<jvmarg value="${jvm.extra.jars}" />
			<arg value="--email" />
			<arg value="${test.email}" />
			<arg value="--outformat" />
			<arg value="out" /> <!-- Output -->
			<arg value="--outfile" />
			<arg value="-" /> <!-- Result output to STDOUT -->
			<arg value="@-" /> <!-- Input ID list from STDIN -->
		</java>
	</target>
	
	<target name="test-emboss_transeq-multifasta-file">
		<echo message="EMBOSS transeq (SOAP): multifasta input file" />
		<java fork="true" failonerror="true" jar="${client.jar}">
			<jvmarg value="${jvm.extra.jars}" />
			<arg value="--email" />
			<arg value="${test.email}" />
			<arg value="--outformat" />
			<arg value="out" /> <!-- Output -->
			<arg value="--outfile" />
			<arg value="-" /> <!-- Result output to STDOUT -->
			<arg value="--multifasta" /> <!-- Multiple fasta sequence input -->
			<arg value="test_data/multi_nuc.tfa" />
		</java>
	</target>
	
	<target name="test-emboss_transeq-multifasta-stdin">
		<echo message="EMBOSS transeq (SOAP): multifasta input from STDIN and result to STDOUT" />
		<java fork="true" failonerror="true" jar="${client.jar}" input="test_data/multi_nuc.tfa">
			<jvmarg value="${jvm.extra.jars}" />
			<arg value="--email" />
			<arg value="${test.email}" />
			<arg value="--outformat" />
			<arg value="out" /> <!-- Output -->
			<arg value="--outfile" />
			<arg value="-" /> <!-- Result output to STDOUT -->
			<arg value="--multifasta" /> <!-- Multiple fasta sequence input -->
			<arg value="-" /> <!-- Multiple fasta sequence from STDIN -->
		</java>
	</target>
	
	<!-- Clean-up -->
	<target name="test-clean-emboss_transeq" description="EMBOSS transeq (SOAP) test clean">
		<delete>
			<fileset dir="." includes="emboss_transeq-**"/>
		</delete>
	</target>
	
</project>
